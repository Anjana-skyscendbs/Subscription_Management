<odoo>
    <record id="action_report_subscriber" model="ir.actions.report">
        <field name="name">Subscriber Report</field>
        <field name="model">subscription.user</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">subscription_managaement.report_subscriber</field>
